{
  "name": "yordle",
  "version-string": "0.0.5",
  "homepage": "https://github.com/yretenai/yordle",
  "description": "Third Party League of Legends File IO Library and Tooling",
  "dependencies": [
    "zstd",
    "zlib",
    "xxhash",
    "flatbuffers",
    "nlohmann-json",
    {
      "name": "curl",
      "features": [
        "openssl",
        "http2"
      ]
    },
    "openssl",
    "fmt",
    {
      "name": "imgui",
      "features": [
        "freetype",
        "docking-experimental"
      ]
    },
    {
      "name": "imgui",
      "features": [
        "dx11-binding",
        "win32-binding"
      ],
      "platform": "windows"
    },
    {
      "name": "imgui",
      "features": [
        "metal-binding"
      ],
      "platform": "osx"
    },
    {
      "name": "imgui",
      "features": [
        "vulkan-binding",
        "opengl3-glad-binding"
      ],
      "platform": "!windows & !osx"
    },
    {
      "name": "glad",
      "features": [
        "extensions",
        "loader",
        "gl-api-latest"
      ]
    },
    "vulkan",
    "opengl"
  ]
}
